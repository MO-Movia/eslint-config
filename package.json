{
  "name": "@modusoperandi/eslint-config",
  "version": "2.1.4",
  "description": "Module containing the lint configuring for MO",
  "main": "main.js",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "ci:build": "echo 'nothing to build'",
    "ci:bom": "npx cyclonedx-npm --ignore-npm-errors --output-format XML --output-file bom.xml",
    "lint": "echo 'nothing to lint'",
    "test:coverage": "echo 'nothing to test'",
    "verify": "npm run lint -- --fix && npm run ci:build && npm run test:coverage && echo 'All Tests Passed!'"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "main.js",
    "main.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MO-Movia/eslint-config"
  },
  "peerDependencies": {
    "eslint": "^9.15.0",
    "angular-eslint": ">=18"
  },
  "peerDependenciesMeta": {
    "angular-eslint": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/parser": "^8.41.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/utils": "^8.41.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.41.0"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "angular-eslint": ">=18.0.0"
  },
  "overrides": {
    "libxmljs2": "^1.0.0"
  },
  "private": false
}
